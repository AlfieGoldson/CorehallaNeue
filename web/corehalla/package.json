{
    "name": "corehalla",
    "version": "0.0.1",
    "dependencies": {
        "@loadable/component": "^5.13.2",
        "@mdi/js": "^5.6.55",
        "@mdi/react": "^1.4.0",
        "corehalla.js": "^0.1.1",
        "cors": "^2.8.5",
        "dayjs": "^1.8.36",
        "express": "^4.17.1",
        "firebase": "^7.21.0",
        "firebase-admin": "^9.2.0",
        "firebase-functions": "^3.11.0",
        "framer-motion": "^2.7.1",
        "next": "^9.5.3",
        "qs": "^6.9.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.1.0",
        "react-router-dom": "^5.2.0",
        "styled-components": "^5.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Corehalla/Corehalla.git"
    },
    "scripts": {
        "dev:client": "next src/client",
        "dev:server": "babel src/server --out-dir dist/server --source-maps --watch",
        "dev": "npm run dev:client & npm run dev:server",
        "preserve": "npm run build",
        "serve": "cross-env NODE_ENV=production firebase serve --only functions,hosting",
        "predeploy": "rimraf dist/ && npm run build",
        "deploy": "cross-env NODE_ENV=production firebase deploy --only functions,hosting"
    },
    "description": "Brawlhalla Stats & Rankings Website",
    "main": "dist/server/index.js",
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/styled-components": "^5.1.3",
        "cross-env": "^7.0.2",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.0.3"
    },
    "keywords": [
        "Brawlhalla",
        "API",
        "Stats",
        "Statistics",
        "Rankings"
    ],
    "author": "Alfie",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Corehalla/Corehalla/issues"
    },
    "homepage": "https://github.com/Corehalla/Corehalla#readme"
